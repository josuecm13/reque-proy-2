        <th>
            @Html.DisplayNameFor(model => model.pasaporte)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.contrasena)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idpaisnacionalidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pasaporte)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idusuario }) |
